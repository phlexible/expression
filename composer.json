{
    "name": "phlexible/expression",
    "description": "phlexible expression component",
    "license": "MIT",
    "type": "library",
    "keywords": ["phlexible"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Component\\Expression\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phlexible\\Component\\ExpressionTests\\": "tests"
        }
    },
    "require": {
        "php": "~7.1",
        "webmozart/expression": "^1.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.5",
        "doctrine/cache": "^1.7",
        "doctrine/orm": "^2.5",
        "phpunit/phpunit": "^7.0",
        "symfony/form": "^3.2|^4.0",
        "symfony/property-access": "^3.2|^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
